{
<% if (!personal) { -%>
  "name": "@attn/<%= name %>",
<% } else { -%>
  "name": "<%= name %>",
<% } -%>
  "version": "0.0.0",
  "description": "<%= description %>",
<% if (open) { -%>
  "license": "MIT",
<% } else { -%>
  "license": "UNLICENSED",
<% } -%>
  "author": {
    "name": "<%= author %>",
    "email": "<%= email %>"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/attn/<%= name %>.git"
  },
  "bugs": {
    "url": "https://github.com/attn/<%= name %>/issues"
  },
  "homepage": "https://github.com/attn/<%= name %>#readme",
<% if (es6) { -%>
  "main": "build.js",
<% } else { -%>
  "main": "index.js",
<% } -%>
  "scripts": {
<% if (es6) { -%>
    "build": "babel src/index.js -out-file build.js",
<% } -%>
    "pretest": "standard",
    "test": "mocha"
  },
  "dependencies": {},
  "devDependencies": {
<% if (es6) { -%>
    "babel": "^5.8.23",
<% } -%>
    "mocha": "^2.2.5",
    "standard": "^5.1.0"
  }
}
